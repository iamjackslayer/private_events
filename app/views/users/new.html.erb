<%= form_for(@user, url: {action: :sign_up})do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.submit "Sign me up!" %>
<% end %>
<br>

<%= form_for(@user, url: {action: :sign_in})do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.submit "Sign me in!" %>
<% end %>s